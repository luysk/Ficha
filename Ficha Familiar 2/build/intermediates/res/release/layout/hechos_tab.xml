<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android" 
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    >
 
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:orientation="vertical" >
        
        <TextView
            android:id="@+id/txtTituloHechos"
            android:layout_width="1280dp"
            android:layout_height="70dp"
            android:layout_gravity="center_horizontal"
            android:gravity="center"
            android:text="@string/titTabHechos"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textSize="40sp" />

    </LinearLayout>    
    
    
       <TableRow
        android:id="@+id/tableTitles"
        android:layout_marginLeft="25dp" 
        android:layout_marginTop="5dp"
        android:layout_weight="0" >

        <TextView
            android:id="@+id/txtCodPer"
            android:layout_marginBottom="5dp"
            android:layout_marginRight="85dp"
            
            android:clickable="false"
            android:text="@string/txtCodPersona"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textSize="30sp"
            android:textStyle="normal" />

        <TextView
            android:id="@+id/txtEspec"
            android:layout_marginBottom="5dp"
            android:layout_marginRight="85dp"
            android:clickable="false"
            android:text="@string/txtEspec"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textSize="30sp"
            android:textStyle="normal"/>
           <!-- android:layout_column="10" -->


        <TextView
            android:id="@+id/txtSaludAct"
            android:layout_marginBottom="5dp"
            android:layout_marginRight="85dp"
            android:text="@string/txtSaludAct"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textSize="30sp"
            android:textStyle="normal"
            android:layout_column="14" />

        <TextView
            android:id="@+id/txtTipoViol"
            android:layout_marginBottom="5dp"
            android:layout_marginRight="85dp"
            android:text="@string/txtTipoViol"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textSize="30sp"
            android:textStyle="normal"
            android:layout_column="16" />
    </TableRow>
    
        <TableRow
        android:id="@+id/tableOptions"
        android:layout_marginLeft="25dp" 
        android:layout_marginBottom="5dp">

        <EditText
            android:id="@+id/editCodPer"
            android:layout_marginRight="85dp"
            android:inputType="number|text" />

        <EditText
            android:id="@+id/editEspec"
            android:ems="10"
            android:inputType="text"
            android:layout_marginRight="110dp"/>
            <!--android:layout_column="10"-->


        <Spinner
            android:id="@+id/spinnerSaludAct"
            android:layout_marginRight="85dp"
            android:layout_column="14" />
            <!--android:nextFocusLeft="@id/txtEspec"
            android:nextFocusRight="@id/txtTipoViol"-->


        <Spinner
            android:id="@+id/spinnerTipoViol"
            android:layout_marginRight="85dp"
            android:layout_column="16" />
        
    </TableRow>   

